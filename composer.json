{
    "name": "knightar/samsara-php",
    "description": "Samsara API SDK",
    "keywords": [
        "samsara",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://www.samsara.com/api-preview",
    "license": "proprietary",
    "authors": [
        {
            "name": "Brandon Lis",
            "homepage": "https://github.com/knightar"
        },
        {
            "name": "Swagger and contributors",
            "homepage": "https://github.com/swagger-api/swagger-codegen"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "Samsara\\Php\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Samsara\\Php\\Client\\" : "test/" }
    }
}
